{
    "version": "0.2.0",
    "configurations": [
      {
        "runtimeExecutable": "/usr/local/bin/node",
        "type": "node",
        "request": "launch",
        "name": "Debug Dev Env",
        "runtimeArgs": [
          "-r",
          "ts-node/register",
          "-r",
          "tsconfig-paths/register"
        ],
        "args": [
          "${workspaceFolder}/src/index.ts"
        ],
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ]
      }
    ]
  }